{
  "expo": {
    "name": "Tabletop Codex",
    "slug": "eogapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#52595d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.tabletopcodex.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#52595d"
      },
      "bundleIdentifier": "com.tabletopcodex.app",
      "package": "com.tabletopcodex.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "react-native-iap"
    ],
    "extra": {
      "eas": {
        "projectId": "e8608f94-25ee-40a6-b40d-678b09faa2dd"
      }
    }
  }
}
